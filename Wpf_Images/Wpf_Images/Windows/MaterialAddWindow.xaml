<Window x:Class="Wpf_Images.Windows.MaterialAddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_Images.Windows"
        mc:Ignorable="d"
        Title="MaterialAddWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock>Title</TextBlock>
            <TextBox x:Name="tbTitle"></TextBox>
            <TextBlock>Group</TextBlock>
            <TextBox x:Name="tbGroup"></TextBox>
            <TextBlock>Information</TextBlock>
            <TextBox x:Name="tbInfo"></TextBox>
            <Button Click="SelectBtn_Click">SELECT</Button>
            <Button Click="AddBtn_Click_1">ADD</Button>
        </StackPanel>
    </Grid>
</Window>
